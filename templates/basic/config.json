{
  "name": "Basic Template",
  "version": "1.0.0",
  "description": "A basic template for mobile applications",
  "features": {
    "navigation": true,
    "notifications": true,
    "search": true,
    "profile": true,
    "gps": false,
    "camera": false,
    "social_sharing": false
  },
  "customization": {
    "colors": {
      "primary": "#007AFF",
